{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "no-multi-spaces": false,
    "no-alert": false,
    "key-spacing": false,
    "curly": false,
    "no-octal-escape": false,
    "no-use-before-define": false,
    "new-cap": false
  },
  "globals": {
    "$": true,
    "angular": true,
    "io": true
  },
  "ecmaFeatures": {
    "modules": true
  }
}